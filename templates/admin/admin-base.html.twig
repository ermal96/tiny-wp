<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>
            {% block title %}Welcome!
            {% endblock %}
        </title>
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon" href="/assets/images/favicon.png" type="image/png" sizes="16x16">
        <link href="/assets/fontawsome/css/all.min.css" rel="stylesheet" type="text/css">
        <link href="/css/main.css" rel="stylesheet" type="text/css">
        <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet"> {% block stylesheets %}{% endblock %}
        </head>
        <body>

            <div id="wrapper">
                {% include "inc/sidebar-navbar.html.twig" %}
                <div style="width:100%" id="content">
                    {% include "inc/top-navbar.html.twig" %}
                    {% block body %}{% endblock %}
                </div>

            </div>
            {{ tinymce_init() }}
            <!-- Bootstrap core JavaScript-->
            <script src="/js/jquery/jquery.min.js"></script>
            <script src="/css/bootstrap/js/bootstrap.bundle.min.js"></script>

            <!-- Core plugin JavaScript-->
            <script src="/js/jquery-easing/jquery.easing.min.js"></script>

            <!-- Custom scripts for all pages-->
            <script src="/js/main.js"></script>
            {% block javascripts %}{% endblock %}
        </body>
    </html>
